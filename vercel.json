{
  "framework": "nextjs",
  "buildCommand": "npm run build",
  "devCommand": "npm run dev",
  "installCommand": "npm install",
  "outputDirectory": ".next",
  "functions": {
    "pages/api/**/*.ts": {
      "maxDuration": 30
    }
  },
  "env": {
    "NEXT_PUBLIC_SITE_URL": "@site_url"
  },
  "build": {
    "env": {
      "NEXT_PUBLIC_SITE_URL": "@site_url"
    }
  },
  "git": {
    "deploymentEnabled": {
      "main": true
    }
  }
}